<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tcp.mapper.TTCPMessageMapper">
	<resultMap id="BaseResultMap" type="com.tcp.bean.TTCPMessage">
		<id column="message_id" property="messageId" jdbcType="VARCHAR" />
		<result column="message_cmd" property="messageCmd" jdbcType="VARCHAR" />
		<result column="message_info" property="messageInfo" jdbcType="VARCHAR" />
		<result column="message_type" property="messageType" jdbcType="INTEGER" />
		<result column="pile_num" property="pileNum" jdbcType="VARCHAR" />
		<result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
	</resultMap>
	<insert id="insert" parameterType="com.tcp.bean.TTCPMessage">
		<selectKey keyProperty="messageId" resultType="java.lang.String"
			order="BEFORE">
			SELECT REPLACE(UUID(),'-','') FROM DUAL
		</selectKey>
		insert into log_tcp_message
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="messageId != null">
				message_id,
			</if>
			<if test="messageCmd != null">
				message_cmd,
			</if>
			<if test="messageInfo != null">
				message_info,
			</if>
			<if test="messageType != null">
				message_type,
			</if>
			<if test="pileNum != null">
				pile_num,
			</if>
			<if test="createTime != null">
				create_time,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="messageId != null">
				#{messageId,jdbcType=VARCHAR},
			</if>
			<if test="messageCmd != null">
				#{messageCmd,jdbcType=VARCHAR},
			</if>
			<if test="messageInfo != null">
				#{messageInfo,jdbcType=VARCHAR},
			</if>
			<if test="messageType != null">
				#{messageType,jdbcType=INTEGER},
			</if>
			<if test="pileNum != null">
				#{pileNum,jdbcType=VARCHAR},
			</if>
			<if test="createTime != null">
				#{createTime,jdbcType=TIMESTAMP},
			</if>
		</trim>
	</insert>
</mapper>